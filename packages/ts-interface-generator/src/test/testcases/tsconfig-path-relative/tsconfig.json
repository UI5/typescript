{
    "compilerOptions": {
        "target": "ES2022",
        "module": "Node16",
        "strict": true,
        "moduleResolution": "Node16",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "paths": {
            "my/*": ["${configDir}/*"]
        }
    }
}